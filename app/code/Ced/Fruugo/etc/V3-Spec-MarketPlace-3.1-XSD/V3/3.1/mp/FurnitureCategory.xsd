        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="autographedBy">
        <xsd:annotation>
          <xsd:documentation>The full name of the person who has autographed this copy.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Autographed by</wm:displayName>
            <wm:group>Nice to Have</wm:group>
            <wm:rank>97000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="features" type="wm:AdditionalFeatures">
        <xsd:annotation>
          <xsd:documentation>List notable features of the item.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Additional Features</wm:displayName>
            <wm:group>Nice to Have</wm:group>
            <wm:rank>102000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="keywords">
        <xsd:annotation>
          <xsd:documentation>Words that people would use to search for this item. Keywords can include synonyms and related terms.</xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Optional"/>
            <wm:displayName>Keywords</wm:displayName>
            <wm:group>Nice to Have</wm:group>
            <wm:rank>103000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="numberOfPanels">
        <xsd:annotation>
          <xsd:documentation>The number of panels the product has as in room dividers and fireplace screens. </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Recommended"/>
            <wm:displayName>Number of Panels</wm:displayName>
            <wm:group>Additional Category Attributes </wm:group>
            <wm:rank>105000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:decimal">
            <xsd:totalDigits value="40"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="seatBackStyle">
        <xsd:annotation>
          <xsd:documentation>Descriptive term for the pattern or form of a seat back component. </xsd:documentation>
          <xsd:appinfo>
            <wm:requiredLevel value="Recommended"/>
            <wm:displayName>Seat Back Style</wm:displayName>
            <wm:group>Additional Category Attributes</wm:group>
            <wm:rank>106000</wm:rank>
          </xsd:appinfo>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="4000"/>
            <xsd:minLength value="1"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element maxOccurs="1" minOccurs="0" name="swatchImages">
        <xsd:complexType>
          <xsd:annotation>
            <xsd:documentation>Enter the swatch image location in &quot;Swatch Image URL&quot; and its corresponding variant attribute name in &quot;Swatch Variant Attribute&quot;. Required for products with visual variations, like color or pattern. List the swatches in the order you recommend they appear on the site.</xsd:documentation>
          </xsd:annotation>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" minOccurs="1" name="swatchImage">
              <xsd:complexType>
                <xsd:all>
                  <xsd:element maxOccurs="1" minOccurs="1" name="swatchVariantAttribute">
                    <xsd:annotation>
                      <xsd:documentation>Attribute name corresponding to the swatch.</xsd:documentation>
                      <xsd:appinfo>
                        <wm:requiredLevel value="Conditionally Required"/>
                        <wm:conditionalAttributes>
                          <wm:conditionalAttributeGroup>
                            <wm:conditionalAttribute name="swatchImageUrl" value="ANY"/>
                          </wm:conditionalAttributeGroup>
                        </wm:conditionalAttributes>
                        <wm:displayName>Swatch Variant Attribute</wm:displayName>
                        <wm:group>Item Variants</wm:group>
                      </xsd:appinfo>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="color"/>
                        <xsd:enumeration value="material"/>
                        <xsd:enumeration value="bedSize"/>
                        <xsd:enumeration value="finish"/>
                        <xsd:enumeration value="pattern"/>
                        <xsd:enumeration value="frameColor"/>
                        <xsd:enumeration value="cushionColor"/>
                        <xsd:enumeration value="shape"/>
                        <xsd:enumeration value="diameter"/>
                        <xsd:enumeration value="mountType"/>
                        <xsd:enumeration value="baseColor"/>
                        <xsd:enumeration value="baseFinish"/>
                        <xsd:enumeration value="sportsTeam"/>
                        <xsd:enumeration value="countPerPack"/>
                        <xsd:enumeration value="count"/>
                        <xsd:enumeration value="character"/>
                        <xsd:enumeration value="assembledProductWidth"/>
                        <xsd:enumeration value="size"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element maxOccurs="1" minOccurs="1" name="swatchImageUrl">
                    <xsd:annotation>
                      <xsd:documentation>URL of the color or pattern swatch image. This is required for products with visual variations and will be shown as a small square on the item page. Recommended resolution is 100 x 100 pixels. Provide the final destination image URL (no-redirects) that is publicly accessible (no username/password required) and ends in a proper image extension such as, .jpg, .png or .gif.</xsd:documentation>
                      <xsd:appinfo>
                        <wm:requiredLevel value="Conditionally Required"/>
                        <wm:conditionalAttributes>
                          <wm:conditionalAttributeGroup>
                            <wm:conditionalAttribute name="swatchVariantAttribute" value="ANY"/>
                          </wm:conditionalAttributeGroup>
                        </wm:conditionalAttributes>
                        <wm:displayName>Swatch Image URL</wm:displayName>
                        <wm:group>Item Variants</wm:group>
                      </xsd:appinfo>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:anyURI"/>
                    </xsd:simpleType>
                  </xsd:element>
                </xsd:all>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
</xsd:schema>
